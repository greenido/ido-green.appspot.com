<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>ido-green</application>
  <version>1</version>

  <threadsafe>true</threadsafe>

  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>

<static-files>
    <include path="/**.gif"  expiration="30d" />
    <include path="/**.png"  expiration="30d" />
    <include path="/**.jpg"  expiration="30d" />
    <include path="/**.jpeg" expiration="30d" />
    <include path="/**.svg"  expiration="30d" />
    <include path="/**.ttf"  expiration="30d" />
    <include path="/**.eot"  expiration="30d" />
    <include path="/**.woff" expiration="30d" />
    <include path="/**.ico"  expiration="30d" />
    <include path="/**.js"   expiration="10d" />
    <include path="/**.css"  expiration="10d" />
    <include path="/**.html" expiration="10d" />
    <include path="/**.json" expiration="10d" />
    <include path="/robots.txt" />
    <include path="/404.html" expiration="30d" />
    <include path="/clock.html" expiration="30d" />
</static-files>

</appengine-web-app>
